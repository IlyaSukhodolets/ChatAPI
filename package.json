{
  "name": "chat-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "prettier:cli": "prettier \"**/*.js\"",
    "prettier": "npm run prettier:cli -- -c",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "eslint --ext .js --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.15",
    "pug": "^3.0.2",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-eslint-plugin": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  }
